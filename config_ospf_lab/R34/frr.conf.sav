frr version 8.5.6
frr defaults traditional
hostname R34
no ipv6 forwarding
service integrated-vtysh-config
!
interface eth0
 ip address 10.30.3.1/24
 ip ospf area 0
exit
!
interface eth1
 ip address 10.30.1.1/24
 ip ospf area 0
exit
!
interface eth2
 ip address 10.20.30.2/24
exit
!
interface lo
 ip address 10.30.34.1/24
exit
!
router ospf
 ospf router-id 10.30.34.1
exit
!
