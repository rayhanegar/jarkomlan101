frr version 8.5.6
frr defaults traditional
hostname R31
no ipv6 forwarding
service integrated-vtysh-config
!
interface eth0
 ip address 10.30.2.2/24
 ip ospf area 0
exit
!
interface eth1
 ip address 10.30.4.2/24
 ip ospf area 0
exit
!
interface eth2
 ip address 10.10.30.2/24
exit
!
interface lo
 ip address 10.30.31.1/24
exit
!
router ospf
 ospf router-id 10.30.31.1
exit
!
