frr version 8.5.6
frr defaults traditional
hostname R22
no ipv6 forwarding
service integrated-vtysh-config
!
ip route 10.200.0.0/16 Null0
!
interface eth0
 ip address 10.200.2.1/24
 ip ospf area 0
exit
!
interface eth1
 ip address 10.200.1.2/24
 ip ospf area 0
exit
!
interface eth2
 ip address 10.10.20.2/24
exit
!
interface lo
 ip address 10.200.22.1/24
exit
!
router bgp 200
 no bgp ebgp-requires-policy
 neighbor 10.10.20.1 remote-as 100
 !
 address-family ipv4 unicast
  network 10.200.0.0/16
  network 10.200.1.0/24
  network 10.200.2.0/24
  network 10.200.3.0/24
  network 10.200.4.0/24
  network 172.17.1.0/24
  neighbor 10.10.20.1 route-map ACCEPT_ALL in
  neighbor 10.10.20.1 route-map ACCEPT_ALL out
 exit-address-family
exit
!
router ospf
 ospf router-id 10.200.22.1
 default-information originate always
exit
!
route-map ACCEPT_ALL permit 10
exit
!
